<div class="d-flex justify-content-between">
  <h1>Articles</h1>
  <h1>index</h1>
</div>
<h2 role="button" aria-pressed="true"><%= link_to "Add?", new_article_path, class: "btn btn-primary btn-lg active btn-block" %></h2>
    <% @articles.each do |article| %>

<div class="card-wraper">

<div class="card">

  <h3 class="card-header"><%= link_to article.title, article_path(article) %></h3>
  <div class="d-flex justify-content-between">
  <p class="card-body"><%= article.content %></p>
    </div>


</div>
    <p  role="button" aria-pressed="true">
    <%= link_to "Delete", article_path(article),
      method: :delete,
      data: { confirm: "Are you sure?" }, class: "btn btn-secondary btn-sm active btn-block" %>
    </p>

</div>
<% end %>
